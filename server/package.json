{
  "name": "urlshortenerserver",
  "version": "1.0.0",
  "description": "server to validate and shorten urls",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npx tsc",
    "start": "npm run clean && npm run build && node server.js",
    "clean": "rm -rf *.js modules/*.js utils/*.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\""
  },
  "keywords": [
    "url",
    "shortUrl"
  ],
  "author": "Matt Gembicky",
  "license": "ISC",
  "engines": {
    "node": "v16.9.1",
    "npm": "8.4.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "md5": "^2.3.0",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/md5": "^2.3.2",
    "@types/node": "^17.0.31",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
