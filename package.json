    "md5": "^2.3.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/md5": "^2.3.2",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.31",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
