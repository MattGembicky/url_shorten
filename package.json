{
  "name": "urlshortenerserver",
  "version": "1.0.0",
  "description": "server to validate and shorten urls",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
